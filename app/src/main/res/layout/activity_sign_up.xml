<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#0f172a"
    tools:context=".sign_up">

    <Button
        android:id="@+id/AsTenant"
        android:layout_width="325dp"
        android:layout_height="64dp"
        android:backgroundTint="#94a3b8"
        android:onClick="sign_up_tenant"
        android:text="Tenant"
        android:textColor="#0f172a "
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/AsAgency"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.891"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/AsAgency"
        android:layout_width="325dp"
        android:layout_height="64dp"
        android:layout_marginBottom="300dp"
        android:backgroundTint="#94a3b8"
        android:onClick="sign_up_agency"
        android:text="Renting Agency"
        android:textColor="#0f172a "
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>